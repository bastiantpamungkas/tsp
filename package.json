{
  "private": true,
  "description": "An example project for NextAuth.js with Next.js",
  "repository": "https://github.com/nextauthjs/next-auth-example.git",
  "bugs": {
    "url": "https://github.com/nextauthjs/next-auth/issues"
  },
  "homepage": "https://next-auth-example.vercel.app",
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "scripts": {
    "postinstall": "npm run prisma:generate",
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "migrate:dev": "npx dotenv -e .env.local -- prisma migrate dev",
    "db:push": "npx dotenv -e .env.local -- prisma db push",
    "migrate:reset": "npx dotenv -e .env.local -- prisma migrate reset",
    "db:seed": "npx dotenv -e .env.local -- prisma db seed",
    "prisma:generate": "npx prisma generate",
    "prisma:studio": "npx dotenv -e .env.local -- prisma studio",
    "production:build": "npx prisma generate && npx prisma migrate deploy && next build"
  },
  "author": "Iain Collins <me@iaincollins.com>",
  "contributors": [
    "Balázs Orbán <info@balazsorban.com>",
    "Nico Domino <yo@ndo.dev>",
    "Lluis Agusti <hi@llu.lu>",
    "Thang Huu Vu <hi@thvu.dev>"
  ],
  "dependencies": {
    "@auth/prisma-adapter": "^1.5.1",
    "@clerk/nextjs": "^5.0.6",
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fontsource/roboto": "^5.0.12",
    "@mui/icons-material": "^5.15.14",
    "@mui/lab": "^5.0.0-alpha.169",
    "@mui/material": "^5.15.14",
    "@mui/x-charts": "^7.2.0",
    "@mui/x-data-grid": "^7.1.1",
    "@mui/x-date-pickers": "^7.1.0",
    "@prisma/client": "^5.11.0",
    "@radix-ui/react-avatar": "^1.0.3",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-navigation-menu": "^1.1.3",
    "@radix-ui/react-slot": "^1.0.2",
    "animate.css": "^4.1.1",
    "class-variance-authority": "^0.7.0",
    "cloudinary": "^2.1.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.274.0",
    "mailtrap": "^3.3.0",
    "mime-types": "^2.1.35",
    "moment": "^2.30.1",
    "next": "latest",
    "next-auth": "beta",
    "next-nprogress-bar": "^2.3.11",
    "nodemailer": "^6.9.13",
    "numeral": "^2.0.6",
    "query-string": "^9.0.0",
    "react": "^18.2.0",
    "react-animation-on-scroll": "^5.1.0",
    "react-custom-scrollbars-2": "^4.5.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.0",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^18",
    "@types/nodemailer": "^6.4.14",
    "@types/numeral": "^2.0.5",
    "@types/react": "^18.2.23",
    "@types/react-dom": "^18.2.8",
    "autoprefixer": "^10.4.15",
    "postcss": "^8.4.29",
    "prisma": "^5.11.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2"
  }
}
